    <div id="contentAnnotation">
        <div id="actionsRegion">

            <textarea class="annotationSpace" id="annotationRegion" name="annotationContent" form="annoSubmit" placeholder="Enter annotations here..."></textarea>

            <form action="/submitAnnotation" style="padding-top:8px"method="post" class="formClass" id="annoSubmit">

                <input type="submit" value="SAVE" class="saveButton-annotation">
                <input id="binder_ID" name="binder_ID" hidden value=<%= binder_ID%>>
            </form>

            <script>
                var obtainedAnno = '<%- annotations %>';
                var finalAnno = '';
                var toConvertToLetter = '';
                for (var aOCC_i = 0; aOCC_i < (obtainedAnno.length / 2); aOCC_i++) {
                    toConvertToLetter = obtainedAnno[2*aOCC_i] + obtainedAnno[(2*aOCC_i)+1];
                    console.log(parseInt('0x' + toConvertToLetter));
                    finalAnno += String.fromCharCode(parseInt('0x' + toConvertToLetter));
                }
                document.getElementById("annotationRegion").value = finalAnno;
            </script>

        </div>
